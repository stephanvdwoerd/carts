function draw_update_spray()
	if (rnd() > 0.5) and stat(34) != 0 then
		local vx = cos(hose.a)
		local vy = sin(hose.a)
		print(a, cam.x, cam.y, 1)

		add(spray, {
			x = hose.endx,
			y = hose.endy,
			a = truck.a,
			vx = vx + rnd(0.1) - 0.05,
			tx = mouse.x,
			ty = mouse.y,
			vy = vy + rnd(0.1) - 0.05,
			tleft = 60,
			c = rnd() > 0.95 and 7 or 12
		})
	end

	for drop in all(spray) do
		drop.tleft -= 1
		drop.x += drop.vx
		drop.y += drop.vy
		pset(drop.x,drop.y, drop.c)
		if drop.tleft <= 0 or dist(drop.x, drop.y, drop.tx, drop.ty) < 5then 
			del(spray, drop)
			add(splashes, {
				x = drop.x,
				y = drop.y,
				r = 2,
				c = drop.c
			})
		end
	end

end


function draw_update_splashes()
	for splash in all(splashes) do
		splash.r += .01

		if splash.c == 7 then
			pset(splash.x,splash.y,splash.c)
		else 
			if splash.r < 3.9 then
				circfill(splash.x,splash.y,splash.r,splash.c)
			else 
				circ(splash.x,splash.y,splash.r,splash.c)
			end
		end

		if splash.r >= 4 then 
			del(splashes, splash)
		end	
	end

end

