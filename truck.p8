	for i in all(puffs) do
		i.t+=1
		i.s= i.ms * (i.t/i.mt)
		
		i.x+=i.vx
		i.y+=i.vy
		 
		if (i.t / i.mt) > 0.2 then
		 fillp(0b1111011011111101)
