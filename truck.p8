function _init()
poke(0x5F2D, 1) -- mouse active

music()
	⧗=0

	mouse = {
		x = 0,
		y = 0
	}

	truck={
		x=50,
		y=100,
		a=0,
		vx=0,
		vy=0,
		spd=0,
		acc=0.01,
		lmultx=0,
		lmulty=1,
		lt={x=0,y=0},
		rt={x=0,y=0},
		lb={x=0,y=0},
		rb={x=0,y=0},
	}

	hose={
		x = truck.x,
		y = truck.y
	}
	
	drag =0.99
	puffs={}

	houses = {}

	trees ={}

	cam = {
		x = 0,
		y = 0
	}


hdist =30
	for u=  0,10 do
 	add(houses, {
 		x = 30+u*hdist,
 		y = 30,
 		layers={64,66,68,70,96,98,100,102,104,106},
 		left= (30+u*hdist)-7,
 		right= (30+u*hdist)+7,
 		top= 30-7,
 		bottom= 30+8,
 		hittop = false,
			hitbottom = false,
			hitleft = false,
			hitright = false,
			housea=0.0,
			hit=false,
			ht=0
 	})
 end
 for u=  0,10 do
 	add(trees, {
 		x = 30+u*hdist,
 		y = 60,
 	 r =rnd(2)-1,
 	 r1 =rnd(2)-1,
 	 r2 =rnd(2)-1,
 		treea=0})

 end
 for v=  0,10 do
 	add(trees, {
 		x = 30+v*hdist,
 		y = 80,
 	 r =rnd(2)-1,
 	 r1 =rnd(2)-1,
 	 r2 =rnd(2)-1,
 		treea=0})

 end
 


end

function _update60()
	⧗+=1
	camera(cam.x, cam.y)
	truck.a =truck.a%1

	-- cam
	cam.y= lerp(truck.y-64, cam.y, 0.95)
	cam.x= lerp(truck.x-64, cam.x, 0.95)
	
	-- mousex
	mouse.x = cam.x + stat(32) 
	mouse.y = cam.y + stat(33) 


	-- hose
	hose.a = atan2(truck.x-mouse.x,truck.y-mouse.y) -0.5
	hose.x = truck.x
	hose.y = truck.y

	-- fire 
	update_fire()

		-- collisions
	for h in all(houses) do
		h.housea=1-atan2(truck.x-h.x,truck.y-h.y)
		h.housea-=0.25
		h.housea=h.housea%1
		
		
		
		if pnr_overlap(truck.lt.x,truck.lt.y,h.left,h.top,h.right,h.bottom)
		or pnr_overlap(truck.rt.x,truck.rt.y,h.left,h.top,h.right,h.bottom)
		or	pnr_overlap(truck.lb.x,truck.lb.y,h.left,h.top,h.right,h.bottom)
		or	pnr_overlap(truck.rb.x,truck.rb.y,h.left,h.top,h.right,h.bottom)
		
		then
			if h.hit == false then
				
				if  h.hittop == true or h.hitbottom == true then
				
				
				truck.a=0.5-truck.a
				end
				if  h.hitright == true or h.hitleft == true then
					truck.a-=truck.a*2
				end
					h.hit = true
				
	
			end
		else
			h.hit = false
			 
		end

	
				if h.	housea >0.125 and h.	housea< 0.375 then
				 h.hitleft = true
				else
					h.hitleft = false
				end
				if h.	housea >0.375 and h.	housea< 0.625
			 then
				 h.hittop = true
				else
					h.hittop = false
				end
				if h.	housea >0.625 and h.	housea< 0.875 then
				 h.hitright = true
				else
					h.hitright = false
				end
				if h.	housea <0.125 or h.	housea> 0.875
			then
				 h.hitbottom = true
			
				else
			h.hitbottom = false
		end
			
			
			
	
		
	end

	
	truck.lt.x = truck.x-5*cos((1-truck.a)-0.14)
	truck.lt.y = truck.y-5*sin((1-truck.a)-0.14)
	truck.rt.x = truck.x-5*cos((1-truck.a)-0.39)
	truck.rt.y = truck.y-5*sin((1-truck.a)-0.39)
	truck.lb.x = truck.x-5*cos((1-truck.a)+0.14)
	truck.lb.y = truck.y-5*sin((1-truck.a)+0.14)
	truck.rb.x = truck.x-5*cos((1-truck.a)+0.39)
	truck.rb.y = truck.y-5*sin((1-truck.a)+0.39)
	
	
	if btn(❎) then
		cam.x+=1
	end
		if btn(🅾️) then
		cam.x-=1
	end
	
	if ⧗ % 4 == 0 then
		makepuff(truck.x,truck.y)
	end

	truck.lmulty=0.3+((1-(truck.y-cam.y)/128)*0.75)

	truck.lmultx=-1*(truck.x-cam.x-64)/64
	

--	truck.a+=0.01

	truck.vx = cos(truck.a-0.25)
	truck.vy	=	sin(truck.a-0.25)
	
	

	
	truck.spd*= drag
	
	truck.x +=truck.vx*truck.spd
	truck.y -=truck.vy*truck.spd
	
	if btn(⬆️) then
		truck.spd+=truck.acc
	end
	if btn(⬇️) then
		truck.spd-=truck.acc
	end
	if btn(➡️) then
		truck.a+=0.01
	end
	if btn(⬅️) then
		truck.a-=0.01
	end
end



